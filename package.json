{
  "version": "3.0.0",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/output",
    "/test"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "scripts": {
    "bootstarp": "yarn install & npm run prettier",
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rollup --config ./rollup.config.js",
    "d": "rollup --config ./rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "w": "rollup --watch --config ./rollup.config.js",
    "prepublishOnly": "npm run prettier & npm run build",
    "test": "jest ./test/jest/",
    "c-demo:all": "node ./test/runner.js all --config ./test/case/demo/config.js",
    "c-demo:all2": "node ./test/runner.js all --config ./test/case/demo/config.js --logLevel 2",
    "c-demo:watch": "node ./test/runner.js watch --config ./test/case/demo/config.js",
    "c-demo:watch2": "node ./test/runner.js watch --config ./test/case/demo/config.js --logLevel 2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-yyl-react": "^0.1.9",
    "jest": "26.6.3",
    "prettier": "^2.0.4",
    "rollup": "^2.38.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3",
    "yyl-cmd-logger": "^0.1.5",
    "yyl-hander": "1.1.6"
  },
  "name": "yyl-seed-webpack",
  "description": "yyl seed webpack",
  "repository": "https://github.com/jackness1208/yyl-seed-webpack.git",
  "author": "liudaojie <liudaojie@yy.com>",
  "license": "MIT",
  "dependencies": {
    "@types/webpack-dev-server": "^3.11.1",
    "chalk": "^4.1.0",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.0.0-beta.0",
    "webpack-merge": "^5.7.3",
    "yyl-base-webpack-config": "^0.1.3",
    "yyl-config-types": "^0.3.17",
    "yyl-os": "^0.11.4",
    "yyl-seed-base": "^0.2.4",
    "yyl-seed-response": "0.2.0",
    "yyl-util": "^2.3.9",
    "yyl-vue2-webpack-config": "^0.1.0"
  }
}
