{
  "name": "yyl-seed-webpack",
  "version": "2.4.0",
  "description": "yyl seed with webpack",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "extends": [
      "yyl-node"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "test/case/",
    "ui-test/",
    "test/__frag/",
    "test/__gitcase/"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "quoteProps": "consistent",
    "trailingComma": "none"
  },
  "scripts": {
    "eslint": "eslint ./",
    "eslint:fix": "eslint ./ --fix",
    "prettier": "prettier --write ./**/*.js",
    "test": "mocha ./test/mocha/ --timeout 1200000",
    "prepublishOnly": "npm run prettier & npm run test:all.case.vue2 & npm run test:all.case.vue2-ts & npm run test:all.case.react-ts",
    "test:all.case.demo": "mocha ./test/mocha/all.case.demo.test.js --timeout 120000",
    "test:all.case.env-test": "mocha ./test/mocha/all.case.env-test.test.js --timeout 120000",
    "test:all.case.local-webpack-config-function": "mocha ./test/mocha/all.case.local-webpack-config-type-function.test.js --timeout 120000",
    "test:all.case.react-github-package": "mocha ./test/mocha/all.case.react-github-package.test.js --timeout 120000",
    "test:all.case.tofu-ui": "mocha ./test/mocha/all.case.tofu-ui.test.js --timeout 120000",
    "test:all.case.vue-js-url": "mocha ./test/mocha/all.case.vue-js-url.test.js --timeout 120000",
    "test:all.case.webpack": "mocha ./test/mocha/all.case.webpack.test.js --timeout 120000",
    "test:all.case.vue2": "mocha ./test/mocha/all.case.vue2.test.js --timeout 120000",
    "test:all.case.vue2-ts": "mocha ./test/mocha/all.case.vue2-ts.test.js --timeout 120000",
    "test:all.case.react-ts": "mocha ./test/mocha/all.case.react-ts.test.js --timeout 120000",
    "test:all.case.yarn": "mocha ./test/mocha/all.case.yarn.test.js --timeout 120000",
    "test:all.git.usercenter": "mocha ./test/mocha/all.git.usercenter.test.js --timeout 120000",
    "test:all.git.yycom_h5_live": "mocha ./test/mocha/all.git.yycom_h5_live.test.js --timeout 120000",
    "test:all.git.yycom_header": "mocha ./test/mocha/all.git.yycom_header.test.js --timeout 120000",
    "tav": "mocha ./test/mocha/all.case.vue2.test.js --timeout 120000",
    "tavt": "mocha ./test/mocha/all.case.vue2-ts.test.js --timeout 120000",
    "tar": "mocha ./test/mocha/all.case.react-ts.test.js --timeout 120000",
    "ui-test": "yyt",
    "frag:all": "node ./test/run.js all --config ../__frag/config.js",
    "frag:watch": "node ./test/run.js watch --config ../__frag/config.js",
    "frag:watch-isCommit": "node ./test/run.js watch --config ../__frag/config.js --isCommit",
    "c-demo:all": "node ./test/run.js all --config ./test/case/demo/config.js",
    "c-demo:watch": "node ./test/run.js watch --config ./test/case/demo/config.js",
    "c-env-test:all": "node ./test/run.js all --config ./test/case/env-test/config.js --proxy --isCommit",
    "c-env-test:watch": "node ./test/run.js watch --config ./test/case/env-test/config.js  --proxy --isCommit",
    "c-webpack:all": "node ./test/run.js all --config ./test/case/webpack/config.js",
    "c-webpack:watch": "node ./test/run.js watch --config ./test/case/webpack/config.js",
    "c-tofu-ui:all": "node ./test/run.js all --config ./test/case/tofu-ui/config.js",
    "c-tofu-ui:watch": "node ./test/run.js watch --config ./test/case/tofu-ui/config.js",
    "c-tofu-ui:watch-isCommit": "node ./test/run.js watch --config ./test/case/tofu-ui/config.js --isCommit",
    "c-vue-js-url:watch": "node ./test/run.js watch --config ./test/case/vue-js-url/config.js",
    "c-vue-js-url:watch-isCommit": "node ./test/run.js watch --config ./test/case/vue-js-url/config.js --isCommit",
    "c-react-github-package:watch": "node ./test/run.js watch --config ./test/case/react-github-package/config.js",
    "c-react-github-package:watch-isCommit": "node ./test/run.js watch --config ./test/case/react-github-package/config.js --isCommit",
    "c-local-wconfig-fn:all": "node ./test/run.js all --config ./test/case/local-webpack-config-type-function/config.js",
    "c-local-wconfig-fn:watch": "node ./test/run.js watch --config ./test/case/local-webpack-config-type-function/config.js",
    "cra": "node ./test/run.js all --config ./test/case/react-ts/config.js",
    "crac": "node ./test/run.js watch --config ./test/case/react-ts/config.js --isCommit",
    "crw": "node ./test/run.js watch --config ./test/case/react-ts/config.js --proxy",
    "crwc": "node ./test/run.js watch --config ./test/case/react-ts/config.js --isCommit",
    "cva": "node ./test/run.js all --config ./test/case/vue2/config.js",
    "cvw": "node ./test/run.js watch --config ./test/case/vue2/config.js",
    "cvwc": "node ./test/run.js watch --config ./test/case/vue2/config.js --isCommit",
    "cvta": "node ./test/run.js all --config ./test/case/vue2-ts/config.js",
    "cvtw": "node ./test/run.js watch --config ./test/case/vue2-ts/config.js",
    "cvtwc": "node ./test/run.js watch --config ./test/case/vue2-ts/config.js --isCommit",
    "caa": "node ./test/run.js all --config ./test/case/after-task/config.js",
    "caw": "node ./test/run.js watch --config ./test/case/after-task/config.js",
    "cawc": "node ./test/run.js watch --config ./test/case/after-task/config.js --isCommit",
    "cda": "node ./test/run.js all --config ./test/case/dev-server-config/config.js",
    "cdw": "node ./test/run.js watch --config ./test/case/dev-server-config/config.js --proxy --logLevel 2",
    "cdwc": "node ./test/run.js watch --config ./test/case/dev-server-config/config.js --isCommit",
    "cha": "node ./test/run.js all --config ./test/case/hmr/config.js",
    "chw": "node ./test/run.js watch --config ./test/case/hmr/config.js --proxy --logLevel 2",
    "chwc": "node ./test/run.js watch --config ./test/case/hmr/config.js --isCommit",
    "cea": "node ./test/run.js all --config ./test/case/err-init/config.js",
    "ceo": "node ./test/run.js all --config ./test/case/err-optimize/config.js",
    "gua": "node ./test/run.js all --config ./test/__gitcase/usercenter/config.js --name pc --logLevel 2",
    "guw": "node ./test/run.js watch --config ./test/__gitcase/usercenter/config.js --name pc --proxy",
    "gha": "node ./test/run.js all --config ./test/__gitcase/yycom_header/yyl.config.js --silent",
    "ghw": "node ./test/run.js watch --config ./test/__gitcase/yycom_header/yyl.config.js --proxy --silent",
    "ghwl": "node ./test/run.js watch --config ./test/__gitcase/yycom_header/yyl.config.js --proxy --logLevel 2 --remote --livereload",
    "gla": "node ./test/run.js all --config ./test/__gitcase/yycom_h5_live/yyl.config.js --logLevel 2",
    "glw": "node ./test/run.js watch --config ./test/__gitcase/yycom_h5_live/yyl.config.js --proxy --logLevel 2",
    "gca": "node ./test/run.js all --config ./test/__gitcase/coop_usercenter/yyl.config.js --logLevel 2",
    "cya": "node ./test/run.js all --config ./test/case/yarn/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-seed-webpack.git"
  },
  "keywords": [
    "yyl",
    "seed",
    "webpack"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/yyl-seed-webpack/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl-seed-webpack#readme",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "acorn": "6.0.5",
    "autoprefixer": "9.7.4",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "browserlist": "^1.0.1",
    "css-loader": "2.0.1",
    "es3ify-webpack-plugin": "0.1.0",
    "file-loader": "2.0.0",
    "happypack": "5.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "isarray": "^2.0.5",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "postcss-px2rem": "0.3.0",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "pug-plain-loader": "1.0.0",
    "raw-loader": "1.0.0",
    "sass": "1.25.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "2.3.5",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.2",
    "url-loader": "1.1.1",
    "watch": "1.0.2",
    "webpack": "4.41.6",
    "webpack-combine-loaders": "2.0.4",
    "webpack-dev-middleware": "3.4.0",
    "webpack-dev-server": "3.10.3",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.2.1",
    "yyl-concat-webpack-plugin": "0.2.10",
    "yyl-copy-webpack-plugin": "0.2.11",
    "yyl-env-pop-webpack-plugin": "0.1.5",
    "yyl-fs": "0.3.2",
    "yyl-os": "0.11.1",
    "yyl-rev-webpack-plugin": "0.1.5",
    "yyl-seed-response": "0.1.1",
    "yyl-sugar-webpack-plugin": "0.1.13",
    "yyl-util": "2.3.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chalk": "3.0.0",
    "eslint": "6.8.0",
    "eslint-config-yyl-node": "0.1.2",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "7.0.1",
    "prettier": "2.0.4",
    "yyl-file-replacer": "0.6.2",
    "yyl-hander": "0.10.0",
    "yyl-print": "0.7.3",
    "yyl-request": "0.1.0",
    "yyl-seed-test-util": "0.4.0",
    "yyl-server": "0.3.6"
  }
}
