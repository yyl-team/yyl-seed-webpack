{
  "version": "3.0.0",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "bin": {
    "yyl-webpack": "bin/index.js"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/output",
    "/test"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "scripts": {
    "bootstarp": "yarn install & npm run prettier",
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rollup --config ./rollup.config.js",
    "d": "rollup --config ./rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "w": "rollup --watch --config ./rollup.config.js",
    "prepublishOnly": "npm run prettier & npm run build",
    "test": "mocha ./test/mocha/ --timeout 120000",
    "test:all.case.demo": "mocha ./test/mocha/all.case.demo.test.js --timeout 120000",
    "test:all.case.env-test": "mocha ./test/mocha/all.case.env-test.test.js --timeout 120000",
    "test:all.case.local-webpack-config-function": "mocha ./test/mocha/all.case.local-webpack-config-type-function.test.js --timeout 120000",
    "test:all.case.react-github-package": "mocha ./test/mocha/all.case.react-github-package.test.js --timeout 120000",
    "test:all.case.tofu-ui": "mocha ./test/mocha/all.case.tofu-ui.test.js --timeout 120000",
    "test:all.case.vue-js-url": "mocha ./test/mocha/all.case.vue-js-url.test.js --timeout 120000",
    "test:all.case.webpack": "mocha ./test/mocha/all.case.webpack.test.js --timeout 120000",
    "test:all.case.vue2": "mocha ./test/mocha/all.case.vue2.test.js --timeout 120000",
    "test:all.case.vue2-ts": "mocha ./test/mocha/all.case.vue2-ts.test.js --timeout 120000",
    "test:all.case.react-ts": "mocha ./test/mocha/all.case.react-ts.test.js --timeout 120000",
    "test:all.case.yarn": "mocha ./test/mocha/all.case.yarn.test.js --timeout 120000",
    "test:all.git.usercenter": "mocha ./test/mocha/all.git.usercenter.test.js --timeout 120000",
    "test:all.git.yycom_h5_live": "mocha ./test/mocha/all.git.yycom_h5_live.test.js --timeout 120000",
    "test:all.git.yycom_header": "mocha ./test/mocha/all.git.yycom_header.test.js --timeout 120000",
    "test:all.git.shenqu_play": "mocha ./test/mocha/all.git.shenqu_play.test.js --timeout 120000",
    "tav": "mocha ./test/mocha/all.case.vue2.test.js --timeout 120000",
    "tavt": "mocha ./test/mocha/all.case.vue2-ts.test.js --timeout 120000",
    "tar": "mocha ./test/mocha/all.case.react-ts.test.js --timeout 120000",
    "c-demo:all": "node ./test/runner.js all --config ./test/case/demo/config.js",
    "c-demo:all2": "node ./test/runner.js all --config ./test/case/demo/config.js --logLevel 2",
    "c-demo:watch": "node ./test/runner.js watch --config ./test/case/demo/config.js",
    "c-demo:watch2": "node ./test/runner.js watch --config ./test/case/demo/config.js --logLevel 2",
    "c-env-test:all": "node ./test/runner.js all --config ./test/case/env-test/config.js --proxy --isCommit",
    "c-env-test:watch": "node ./test/runner.js watch --config ./test/case/env-test/config.js  --proxy --isCommit",
    "c-webpack:all": "node ./test/runner.js all --config ./test/case/webpack/config.js",
    "c-webpack:watch": "node ./test/runner.js watch --config ./test/case/webpack/config.js",
    "c-tofu-ui:all": "node ./test/runner.js all --config ./test/case/tofu-ui/config.js",
    "c-tofu-ui:watch": "node ./test/runner.js watch --config ./test/case/tofu-ui/config.js",
    "c-tofu-ui:watch-isCommit": "node ./test/runner.js watch --config ./test/case/tofu-ui/config.js --isCommit",
    "c-vue-js-url:watch": "node ./test/runner.js watch --config ./test/case/vue-js-url/config.js",
    "c-vue-js-url:watch-isCommit": "node ./test/runner.js watch --config ./test/case/vue-js-url/config.js --isCommit",
    "c-react-github-package:watch": "node ./test/runner.js watch --config ./test/case/react-github-package/config.js",
    "c-react-github-package:watch-isCommit": "node ./test/runner.js watch --config ./test/case/react-github-package/config.js --isCommit",
    "c-local-wconfig-fn:all": "node ./test/runner.js all --config ./test/case/local-webpack-config-type-function/config.js",
    "c-local-wconfig-fn:watch": "node ./test/runner.js watch --config ./test/case/local-webpack-config-type-function/config.js",
    "cra": "node ./test/runner.js all --config ./test/case/react-ts/config.js",
    "crac": "node ./test/runner.js all --config ./test/case/react-ts/config.js --isCommit",
    "crar": "node ./test/runner.js all --config ./test/case/react-ts/config.js --remote",
    "crw": "node ./test/runner.js watch --config ./test/case/react-ts/config.js --proxy",
    "crwc": "node ./test/runner.js watch --config ./test/case/react-ts/config.js --isCommit",
    "cva": "node ./test/runner.js all --config ./test/case/vue2/config.js",
    "cvw": "node ./test/runner.js watch --config ./test/case/vue2/config.js",
    "cvwc": "node ./test/runner.js watch --config ./test/case/vue2/config.js --isCommit",
    "cvta": "node ./test/runner.js all --config ./test/case/vue2-ts/config.js",
    "cvtw": "node ./test/runner.js watch --config ./test/case/vue2-ts/config.js",
    "cvtwc": "node ./test/runner.js watch --config ./test/case/vue2-ts/config.js --isCommit",
    "caa": "node ./test/runner.js all --config ./test/case/after-task/config.js",
    "caw": "node ./test/runner.js watch --config ./test/case/after-task/config.js",
    "cawc": "node ./test/runner.js watch --config ./test/case/after-task/config.js --isCommit",
    "cda": "node ./test/runner.js all --config ./test/case/dev-server-config/config.js",
    "cdw": "node ./test/runner.js watch --config ./test/case/dev-server-config/config.js --proxy --logLevel 2",
    "cdwc": "node ./test/runner.js watch --config ./test/case/dev-server-config/config.js --isCommit",
    "cha": "node ./test/runner.js all --config ./test/case/hmr/config.js",
    "chw": "node ./test/runner.js watch --config ./test/case/hmr/config.js --proxy --logLevel 2",
    "chwc": "node ./test/runner.js watch --config ./test/case/hmr/config.js --isCommit",
    "cea": "node ./test/runner.js all --config ./test/case/err-init/config.js",
    "ceo": "node ./test/runner.js all --config ./test/case/err-optimize/config.js",
    "cesa": "node ./test/runner.js all --config ./test/case/es6/config.js",
    "gua": "node ./test/runner.js all --config ./test/__gitcase/usercenter/yyl.config.js --logLevel 2",
    "guw": "node ./test/runner.js watch --config ./test/__gitcase/usercenter/yyl.config.js --proxy",
    "gha": "node ./test/runner.js all --config ./test/__gitcase/yycom_header/yyl.config.js --silent",
    "ghw": "node ./test/runner.js watch --config ./test/__gitcase/yycom_header/yyl.config.js --proxy --silent",
    "ghwl": "node ./test/runner.js watch --config ./test/__gitcase/yycom_header/yyl.config.js --proxy --logLevel 2 --remote --livereload",
    "gla": "node ./test/runner.js all --config ./test/__gitcase/yycom_h5_live/yyl.config.js --logLevel 2",
    "glw": "node ./test/runner.js watch --config ./test/__gitcase/yycom_h5_live/yyl.config.js --proxy --logLevel 2",
    "gca": "node ./test/runner.js all --config ./test/__gitcase/coop_usercenter/yyl.config.js --logLevel 2",
    "gsa": "node ./test/runner.js all --config ./test/__gitcase/shenqu_play/yyl.config.js --logLevel 2",
    "gsw": "node ./test/runner.js watch --config ./test/__gitcase/shenqu_play/yyl.config.js --proxy --logLevel 2 --tips",
    "cya": "node ./test/runner.js all --config ./test/case/yarn/config.js",
    "caqa": "node ./test/runner.js all --config ./test/case/aquaman/config.js",
    "caqw": "node ./test/runner.js watch --config ./test/case/aquaman/config.js --proxy"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "chai": "^4.3.4",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-yyl-react": "^0.1.9",
    "mocha": "^8.3.2",
    "prettier": "^2.0.4",
    "rollup": "^2.38.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3",
    "yyl-cmd-logger": "^0.1.5",
    "yyl-file-replacer": "^0.6.4",
    "yyl-hander": "1.2.2",
    "yyl-request": "^0.1.1",
    "yyl-seed-test-util": "^0.4.0"
  },
  "name": "yyl-seed-webpack",
  "description": "yyl seed webpack",
  "repository": "https://github.com/jackness1208/yyl-seed-webpack.git",
  "author": "liudaojie <liudaojie@yy.com>",
  "license": "MIT",
  "dependencies": {
    "@types/webpack-dev-server": "^3.11.1",
    "chalk": "^4.1.0",
    "querystring-es3": "^0.2.1",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "yyl-base-webpack-config": "^0.1.14",
    "yyl-config-types": "^0.4.3",
    "yyl-os": "^0.11.4",
    "yyl-seed-base": "^0.2.6",
    "yyl-seed-response": "0.2.0",
    "yyl-util": "^2.3.9",
    "yyl-vue2-webpack-config": "^0.1.1"
  }
}
